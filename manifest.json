{
  "manifest_version": 2,
  "name": "FB Off Trend",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action":{
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches":[
        "http://www.facebook.com/",
        "https://www.facebook.com/*"
      ],
      "js": [
        "jquery.min.js",
        "content.js"
      ]
    }
  ],
  "background":{
    "scripts": ["background.js"],
    "persistent": true
  },
  "web_accessible_resources":[
    "placeholder.html",
    "images/*"
  ]

}
